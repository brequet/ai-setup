{
  "name": "@brequet/ai-setup",
  "version": "0.1.0",
  "description": "CLI tool for managing AI agent catalogs, skills, and MCP configurations",
  "type": "module",
  "bin": {
    "bre-ai-setup": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "lint": "oxlint src",
    "lint:fix": "oxlint --fix src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "ai",
    "opencode",
    "mcp",
    "skills"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
